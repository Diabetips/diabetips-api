{

    "fileHeaderComment.parameter": {
        "*": {
            "prefixBeg": "/*!",
            "prefixMid": "**",
            "prefixEnd": "*/"
        }
    },
    "fileHeaderComment.template": {
        "*": [
            "${prefixBeg}",
            "${prefixMid} Copyright ${year} Diabetips",
            "${prefixMid}",
            "${prefixMid} All rights reserved",
            "${prefixMid}",
            "${prefixMid} Created by ${author} on ${date}",
            "${prefixEnd}"
        ]
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.exclude": {
        "**/build": true,
        "**/docs": true,
        "**/logs": true,
        "**/node_modules/**": true,
        "bin": true
    }
}
